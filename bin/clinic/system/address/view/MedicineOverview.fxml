<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clinic.system.address.view.MedicineOverviewController">
   <children>
      <TableView fx:id="DeleteButton" layoutX="21.0" layoutY="72.0" prefHeight="499.0" prefWidth="757.0">
        <columns>
          <TableColumn fx:id="MedicineName" prefWidth="316.0" text="ชื่อเวชภัณฑ์" />
          <TableColumn fx:id="MedicineID" minWidth="0.0" prefWidth="187.0" text="รหัสเวชภัณฑ์" />
            <TableColumn fx:id="MedicineQuatity" minWidth="0.0" prefWidth="133.0" text="จำนวนเวชภัณฑ์" />
            <TableColumn fx:id="MedicinePrice" prefWidth="120.0" text="ราคา (บาท)" />
        </columns>
      </TableView>
      <MenuButton fx:id="MedicineButton" layoutX="21.0" layoutY="27.0" mnemonicParsing="false" text="ระบบเวชภัณฑ์">
        <items>
          <MenuItem mnemonicParsing="false" text="เรียงข้อมูลจาก..." />
          <MenuItem mnemonicParsing="false" text="ตั้งค่า" />
        </items>
      </MenuButton>
      <Button fx:id="AddMedicineButton" layoutX="637.0" layoutY="27.0" mnemonicParsing="false" onAction="#handleADD" text="Add" />
      <Button fx:id="EditMedicineButton" layoutX="682.0" layoutY="27.0" mnemonicParsing="false" text="Edit" />
      <Button fx:id="DeleteMedicineButton" layoutX="727.0" layoutY="27.0" mnemonicParsing="false" text="Delete" />
      <TextField fx:id="SearchBox" layoutX="133.0" layoutY="27.0" prefHeight="25.0" prefWidth="213.0" />
      <Button fx:id="SearchButton" layoutX="354.0" layoutY="27.0" mnemonicParsing="false" text="Search" />
   </children>
</AnchorPane>
