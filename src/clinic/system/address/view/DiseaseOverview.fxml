<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clinic.system.address.view.DiseaseOverviewController">
   <children>
      <TableView fx:id="diseaseList" editable="true" layoutX="21.0" layoutY="72.0" prefHeight="460.0" prefWidth="757.0">
         <columns>
            <TableColumn fx:id="diseaseFname" prefWidth="226.0" text="ชื่อ" />
            <TableColumn fx:id="diseaseLName" minWidth="0.0" prefWidth="188.0" text="นามสกุล" />
            <TableColumn fx:id="diseaseID" minWidth="0.0" prefWidth="157.0" text="รหัสคนไข้" />
            <TableColumn fx:id="diseaseName" prefWidth="167.0" text="โรคที่พบ" />
         </columns>
      </TableView>
      <TextField fx:id="SearchBox" layoutX="63.0" layoutY="27.0" prefHeight="25.0" prefWidth="171.0" />
      <Button fx:id="SearchButton" layoutX="241.0" layoutY="27.0" mnemonicParsing="false" onAction="#handleSearch" text="Search" />
      <Button fx:id="DeleteButton" layoutX="727.0" layoutY="27.0" mnemonicParsing="false" onAction="#handleDelete" text="Delete" />
      <ImageView fitHeight="32.0" fitWidth="32.0" layoutX="21.0" layoutY="24.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/CMS-disease.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
